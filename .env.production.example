# Production Environment Variables Template
# Copy this file to .env.production and replace values with your actual configuration

NODE_ENV=production

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-supabase-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Application Configuration
NEXT_PUBLIC_APP_URL=https://alumni.yourdomain.com

# For server-side only (not exposed to the browser)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional: Analytics and monitoring
NEXT_PUBLIC_ANALYTICS_ID=

# Optional: Email service configuration
EMAIL_SERVICE_API_KEY=
EMAIL_FROM_ADDRESS=alumni@yourdomain.com 