# Production Environment Variables
NODE_ENV=production

# IMPORTANT: These are placeholder values!
# You must set the actual Supabase credentials in your Vercel project settings
# Go to: https://vercel.com/ashwin-s-tech-solution/v0-new-project-j3o2rh6forn/settings/environment-variables
NEXT_PUBLIC_SUPABASE_URL=https://your-real-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-real-anon-key

# Application Configuration
NEXT_PUBLIC_APP_URL=https://ametalumni.in

# For server-side only (not exposed to the browser)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Disable mock data in production
USE_MOCK_DATA=false

# Optional: Analytics and monitoring
NEXT_PUBLIC_ANALYTICS_ID=

# Optional: Email service configuration
EMAIL_SERVICE_API_KEY=
EMAIL_FROM_ADDRESS=alumni@yourdomain.com

# New variables from the code block
NEXT_PUBLIC_USE_MOCK_DATA=true
NEXT_DISABLE_ESLINT=1
DISABLE_ESLINT_PLUGIN=true
NEXT_TELEMETRY_DISABLED=1
NEXT_PUBLIC_VERCEL_ENV=production
SKIP_API_ROUTES=true 